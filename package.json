{
  "name": "@travi/scaffolder-updates-test",
  "description": "test of current project scaffolder updates",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "repository": "travi-test/scaffolder-updates-test",
  "bugs": "https://github.com/travi-test/scaffolder-updates-test/issues",
  "homepage": "https://npm.im/@travi/scaffolder-updates-test",
  "runkitExampleFilename": "./example.js",
  "exports": "./lib/index.mjs",
  "main": "./lib/index.mjs",
  "sideEffects": false,
  "scripts": {
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "prepack": "run-s build",
    "pregenerate:md": "run-s build",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run",
    "lint:lockfile": "lockfile-lint",
    "prepare": "husky",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:peer": "npm ls >/dev/null",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s 'lint:js -- --fix'",
    "lint:publish": "publint --strict",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*"
  },
  "files": [
    "example.js",
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@travi/any": "3.1.3",
    "@travi/eslint-config": "5.0.12",
    "c8": "10.1.3",
    "commitlint-config-travi": "1.4.59",
    "cross-env": "10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "jest-when": "3.7.0",
    "lockfile-lint": "4.14.1",
    "npm-run-all2": "8.0.4",
    "publint": "0.3.14",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.8",
    "remark-toc": "9.0.0",
    "remark-usage": "11.0.1",
    "rimraf": "6.0.1",
    "rollup": "4.52.4",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "3.2.4"
  }
}
